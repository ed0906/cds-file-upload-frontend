# microservice specific routes

GET         /start                              controllers.StartController.displayStartPage
GET         /upload                             controllers.FileUploadController.displayFileInfoForm
POST        /upload                             controllers.FileUploadController.handleFileInfoForm
GET         /upload/size                        controllers.FileUploadController.displayFileSizeForm
POST        /upload/size                        controllers.FileUploadController.handleFileSizeForm
GET         /upload/:batchId/receipts           controllers.FileUploadController.displayConfirmationPage(batchId)
GET         /upload/:batchId/:fileNumber        controllers.FileUploadController.displayFileUploadForm(batchId, fileNumber: Int)

GET         /unauthorised                       controllers.UnauthorisedController.onPageLoad

GET         /mrn-entry                          controllers.MrnEntryController.onPageLoad
POST        /mrn-entry                          controllers.MrnEntryController.onSubmit

GET         /upload/warning                     controllers.FileWarningController.onPageLoad
